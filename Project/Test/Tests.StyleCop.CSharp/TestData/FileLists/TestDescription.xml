      <Violation Section="Root.TestFile2.TestFile2" LineNumber="12" Rule="AccessModifierMustBeDeclared" />
      <Violation Section="Root" LineNumber="1" Rule="FileHeaderFileNameDocumentationMustMatchTypeName" />
    </ExpectedViolations>
  </Test>

